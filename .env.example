# Node.js Backend (Nestjs)
PORT=3000

# PostgreSQL Database
DB_EXTERNAL_PORT=5432
DB_DATABASE=pauta_cidada
DB_USERNAME=postgres
DB_PASSWORD=postgres123
DB_HOST=database
DB_PORT=5432

# Frontend
VITE_API_URL=http://localhost:3000

# Python Backend (FastAPI)
PYTHON_BACKEND_PORT=8000

# OpenAI Configuration
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_API_KEY_HERE

# Google Cloud BigQuery Configuration
GOOGLE_CLOUD_PROJECT=your-project-id

# OPÇÃO 1: Credenciais como JSON (RECOMENDADO PARA PORTAINER/DOCKER)
# Cole o conteúdo do seu credentials.json em uma única linha
# Para gerar: cat credentials.json | jq -c '.'
GOOGLE_APPLICATION_CREDENTIALS_JSON='{"type":"service_account","project_id":"your-project","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"...@....iam.gserviceaccount.com",...}'

# OPÇÃO 2: Path para arquivo (DESENVOLVIMENTO LOCAL - descomente se preferir)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_BUCKET_NAME=proposition-pdfs

# Database URL (Supabase PostgreSQL)
# IMPORTANTE PARA PORTAINER/PRODUÇÃO:
# Se tiver problemas de conexão em Docker/Portainer, use o Connection Pooler (porta 6543):
# DATABASE_URL=postgresql+asyncpg://postgres:your-password@aws-0-us-east-1.pooler.supabase.com:6543/postgres
# 
# Para desenvolvimento local, pode usar a conexão direta (porta 5432):
DATABASE_URL=postgresql+asyncpg://postgres:your-password@db.your-project.supabase.co:5432/postgres
# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_BUCKET_NAME=proposition-pdfs

# Database URL para SQLAlchemy (Supabase PostgreSQL)
DATABASE_URL=postgresql+asyncpg://postgres:your-password@db.your-project.supabase.co:5432/postgres
